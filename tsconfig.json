{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ],
      "@/screens/*": [
        "./screens/*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/utils/*": [
        "./utils/*"
      ],
      "@/constants/*": [
        "./constants/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@gs": [
        "./components/ui/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "tailwind.config": [
        "./tailwind.config.js"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "app/**/*.ts",
    "app/**/*.tsx",
    "constants/**/*.ts",
    "constants/**/*.tsx",
    "assets/**/*.ts",
    "screens/**/*.ts",
    "screens/**/*.tsx",
    "components/**/*.ts",
    "components/**/*.tsx",
    "lib/**/*.ts",
    "lib/**/*.tsx"
, "screens/auth/reset-password/EnterNewPasswordstsx"  ]
}